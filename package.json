{
  "name": "rocket-react-redux",
  "version": "0.0.4",
  "description": "Alternative React binding for Redux made in Typescript",
  "main": "index.js",
  "scripts": {
    "deploy": "yarn publish",
    "test:eslint": "eslint --ext .ts,.tsx ./",
    "test:lint": "tsc --noEmit",
